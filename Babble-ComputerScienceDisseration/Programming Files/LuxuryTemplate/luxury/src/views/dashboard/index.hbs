    - ../assets/vendor/flot/jquery.flot.tooltip.min.js
    - ../assets/vendor/flot/jquery.flot.categories.min.js
    - ../assets/vendor/bower_components/jquery.easy-pie-chart/dist/jquery.easypiechart.min.js
    - ../assets/vendor/bower_components/peity/jquery.peity.min.js
    - ../assets/vendor/owl-carousel/owl.carousel.min.js

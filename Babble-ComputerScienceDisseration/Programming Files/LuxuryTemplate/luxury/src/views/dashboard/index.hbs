      <div class="row">
        <div class="col-lg-8">
          <div class="card">
            <header class="card-header">
              <h6 class="card-heading">Recent Orders</h6>
              {{> card-toolbar}}
            </header>
            <div>
              <div class="d-flex justify-content-end p-4 mb-4">
                <label>
                  <span class="mr-2">Live</span>
                  <input type="checkbox" data-plugin="switchery" data-color="{{colors.success}}" data-size="small" checked />
                </label>
              </div>
              <div id="dash1-flotchart-1" style="width: 100%; height: 320px"></div>
            </div>
          </div><!-- /.card -->
        </div><!-- /.col -->

